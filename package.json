{
  "name": "serverless-seed",
  "version": "1.0.0",
  "description": "NextFaze Serverless Seed. Bringing sanity to your serverless development experience.",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "clean": "rm -rf lib",
    "deploy:production": "npm run build && npm run serverless -- deploy --stage production",
    "deploy:staging": "npm run build && npm run serverless -- deploy --stage staging",
    "mocha": "node_modules/.bin/mocha",
    "pretest": "npm run build",
    "test": "npm run mocha lib/**/*.spec.js",
    "serverless": "node_modules/.bin/serverless",
    "sls": "node_modules/.bin/sls",
    "tsc": "node_modules/.bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.nextfaze.com:nextfaze/serverless-seed.git"
  },
  "keywords": [
    "amazon",
    "aws",
    "serverless",
    "typescript",
    "node"
  ],
  "author": "Michael Marner <mmarner@nextfaze.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-sdk": "0.0.42",
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.39",
    "@types/moment-timezone": "^0.2.34",
    "@types/node": "^7.0.5",
    "@types/sparkpost": "^1.3.1",
    "aws-sdk": "^2.11.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "serverless": "^1.6.1",
    "typescript": "^2.1.6"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "sparkpost": "^2.1.2"
  }
}
